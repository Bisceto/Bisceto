import { show, stack_frac, heart, sail } from "rune";

function stackn(n, rune) { 
    display(n, "current n: " );
    //base case of n===1 to stop recursion from happening over again
    return n === 1 ? rune : stack_frac(1 / n, 
                      rune,
                      stackn(n - 1, rune)); 
}

show(stackn(, heart));